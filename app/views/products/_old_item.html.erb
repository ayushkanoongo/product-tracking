<tr>
  <td>
    <%= f.input_field :_destroy, as: :hidden %>
    <%= link_to 'Delete', '#', class: 'remove_record' %>
  </td>
  <td><%= f.input :serial_number, label: false, disabled: false %></td>
  <td><%= f.select :item_type, [ 'M.O.S (1.5 CuM)', 'M.O.B (7.0 CuM)' ], :prompt => 'Select One', disabled: false %></td>
  <td><%= f.select :status, [ 'Yes', 'No' ], :prompt => 'Select One' %></td>
</tr>

<div class="form-actions">
  <%= link_to_add_row('Add Item', ff, :items, class: 'btn btn-primary') %>
</div>